<suite name="Test Suite for Publisher 7" parallel="methods" thread-count="1" preserve-order="true">

  	<parameter name="Environment" value="PROD"></parameter>
  	
  	<test name="All Web Tests">

		<!-- Enter group name for specific test scripts, options are "full" or "smoke" -->
		<groups>
	 		<run>
	 			<include name="full"/>
	 		</run>
		</groups>

		<!--Add individual packages you wanted to execute below-->
   		<packages>
   			<package name="com.nbcuni.test.publisher.tests.*"/>
		</packages>

	</test>
	
	
	
</suite>